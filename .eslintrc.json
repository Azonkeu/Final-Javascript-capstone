{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "camelcase": "off",
      "quotes": "off",
      "indent": "off",
      "no-spaced-func": "off",
      "no-use-before-define": "off",
      "no-trailing-spaces": "off",
      "func-call-spacing": "off",
      "quote-props": "off",
      "space-before-blocks": "off",
      "padded-blocks": "off",
      "arrow-parens": "off",
      "import/extensions": [ 1, {
        "js": "always", "json": "always"
      }]
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }